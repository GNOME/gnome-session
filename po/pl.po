#: data/gnome.desktop.in.in:2 data/gnome.session.desktop:2
msgid "GNOME"
msgstr "GNOME"

#: data/gnome.desktop.in.in:3 data/gnome-wayland.desktop.in.in:3
#: data/gnome-xorg.desktop.in.in:3
msgid "This session logs you into GNOME"
msgstr "Ta sesja loguje użytkownika do środowiska GNOME"

#: data/gnome-wayland.desktop.in.in:2
msgid "GNOME on Wayland"
msgstr "GNOME (Wayland)"

#: data/gnome-xorg.desktop.in.in:2
msgid "GNOME on Xorg"
msgstr "GNOME (Xorg)"

#: data/org.gnome.SessionManager.gschema.xml:5
msgid "Logout prompt"
msgstr "Potwierdzenie przed wylogowaniem"

#: data/org.gnome.SessionManager.gschema.xml:6
msgid "If enabled, gnome-session will prompt the user before ending a session."
msgstr ""
"Czy gnome-session ma pytać użytkownika o potwierdzenie przed zakończeniem "
"sesji."

#: data/org.gnome.SessionManager.gschema.xml:10
msgid "Show the fallback warning"
msgstr "Ostrzeżenie o przywróceniu"

#: data/org.gnome.SessionManager.gschema.xml:11
msgid ""
"If enabled, gnome-session will display a warning dialog after login if the "
"session was automatically fallen back."
msgstr ""
"Czy gnome-session ma wyświetlać okno z ostrzeżeniem po zalogowaniu, jeśli "
"sesja została automatycznie przywrócona do poprzedniej."

#: gnome-session/gsm-manager.c:709 gnome-session/gsm-manager.c:1040
msgid "Not responding"
msgstr "Brak odpowiedzi"

#: gnome-session/gsm-util.c:167
msgid "_Log out"
msgstr "_Wyloguj się"

#: gnome-session/leader-main.c:181 gnome-session/service-main.c:148
msgid "Session to use"
msgstr "Używana sesja"

#: gnome-session/leader-main.c:181 gnome-session/service-main.c:148
msgid "SESSION_NAME"
msgstr "NAZWA_SESJI"

#: gnome-session/leader-main.c:182
msgid "Enable debugging code"
msgstr "Uruchomienie kodu debugowania"

#: gnome-session/leader-main.c:183
msgid "Version of this application"
msgstr "Wersja tego programu"

#: gnome-session/leader-main.c:184
msgid "This option is ignored"
msgstr "Ta opcja jest ignorowana"

#: gnome-session/leader-main.c:185
msgid "Don't re-exec into a login shell"
msgstr "Bez ponownego wchodzenia do powłoki logowania"

#: gnome-session/leader-main.c:203 gnome-session/service-main.c:157
msgid " — the GNOME session manager"
msgstr " — menedżer sesji środowiska GNOME"

#: tools/gnome-session-ctl.c:247
msgid "Start gnome-session-shutdown.target"
msgstr "Rozpoczyna gnome-session-shutdown.target"

#: tools/gnome-session-ctl.c:248
msgid ""
"Start gnome-session-shutdown.target when receiving EOF or a single byte on "
"stdin"
msgstr ""
"Rozpoczyna gnome-session-shutdown.target po otrzymaniu EOF lub pojedynczego "
"bajtu na standardowym wejściu"

#: tools/gnome-session-ctl.c:249
msgid "Signal initialization done to gnome-session"
msgstr "Sygnalizuje gnome-session, że inicjacja została ukończona"

#: tools/gnome-session-ctl.c:250
msgid "Restart dbus.service if it is running"
msgstr "Uruchamia usługę dbus.service ponownie, jeśli jest uruchomiona"

#: tools/gnome-session-ctl.c:251
msgid ""
"Run from ExecStopPost to start gnome-session-shutdown.target on service "
"failure"
msgstr ""
"Uruchamia z ExecStopPost, aby rozpocząć gnome-session-shutdown.target po "
"niepowodzeniu usługi"

#: tools/gnome-session-ctl.c:281
msgid "Program needs exactly one parameter"
msgstr "Program wymaga dokładnie jednego parametru"

#: tools/gnome-session-inhibit.c:69
#, c-format
msgid "Unknown inhibit argument: %s"
msgstr "Nieznany parametr wstrzymywania: %s"

#: tools/gnome-session-inhibit.c:289
#, c-format
msgid "Failed to execute %s: %m\n"
msgstr "Wykonanie polecenia „%s” się nie powiodło: %m\n"

#: tools/gnome-session-inhibit.c:361
msgid "Show program version"
msgstr "Wyświetla wersję programu"

#: tools/gnome-session-inhibit.c:362
msgid "The application id to use when inhibiting (optional)"
msgstr "Identyfikator programu używany podczas wstrzymywania (opcjonalnie)"

#: tools/gnome-session-inhibit.c:362
msgid "ID"
msgstr "IDENTYFIKATOR"

#: tools/gnome-session-inhibit.c:363
msgid "The reason for inhibiting (optional)"
msgstr "Powód wstrzymania (opcjonalnie)"

#: tools/gnome-session-inhibit.c:363
msgid "REASON"
msgstr "POWÓD"

#: tools/gnome-session-inhibit.c:364
msgid "Add action to inhibit (repeatable)"
msgstr "Dodaje działanie do wstrzymania (można podać więcej niż jeden raz)"

#: tools/gnome-session-inhibit.c:364
msgid "ACTION"
msgstr "DZIAŁANIE"

#: tools/gnome-session-inhibit.c:365
msgid "Do not launch COMMAND and wait forever instead"
msgstr "Bez wykonywania POLECENIA i oczekiwanie bez ograniczenia"

#: tools/gnome-session-inhibit.c:366
msgid "List the existing inhibitions, and exit"
msgstr "Wyświetla listę istniejących wstrzymywań i kończy działanie"

#: tools/gnome-session-inhibit.c:378
msgid "COMMAND"
msgstr "POLECENIE"

#: tools/gnome-session-inhibit.c:384
msgid "Execute COMMAND while inhibiting some session functionality."
msgstr "Wykonuje POLECENIE podczas wstrzymywania części funkcjonalności sesji."

#: tools/gnome-session-inhibit.c:385
msgid ""
"Valid ACTION values are: logout, switch-user, suspend, idle, and automount.\n"
"If no --inhibit options are specified, idle is assumed."
msgstr ""
"Prawidłowe wartości DZIAŁANIA: logout, switch-user, suspend, idle "
"i automount.\n"
"Jeśli nie podano opcji --inhibit, to przyjmowana jest wartość „idle”."

#: tools/gnome-session-inhibit.c:409
msgid "Neither COMMAND nor --inhibit-only were specified."
msgstr "Nie podano POLECENIA ani opcji --inhibit-only."

#: tools/gnome-session-quit.c:50
msgid "Log out"
msgstr "Wylogowanie"

#: tools/gnome-session-quit.c:51
msgid "Power off"
msgstr "Wyłączenie"

#: tools/gnome-session-quit.c:52
msgid "Reboot"
msgstr "Ponowne uruchomienie"

#: tools/gnome-session-quit.c:53
msgid "Ignoring any existing inhibitors"
msgstr "Ignorowanie istniejących mechanizmów wstrzymujących"

#: tools/gnome-session-quit.c:54
msgid "Don’t prompt for user confirmation"
msgstr "Bez pytania o potwierdzenie użytkownika"

#: tools/gnome-session-quit.c:88 tools/gnome-session-quit.c:102
msgid "Could not connect to the session manager"
msgstr "Nie można połączyć się z menedżerem sesji"

#: tools/gnome-session-quit.c:198
msgid "Program called with conflicting options"
msgstr "Wywołano program ze sprzecznymi opcjami"
